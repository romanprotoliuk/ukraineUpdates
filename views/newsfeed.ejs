<div class="body-wrapper">
  <div class="top-body-wrap">
    <div>
      <h1>Newsfeed</h1>
    </div>

    <div>
      <a href="/users/noteform">Document a war crime</a>
    </div>
  </div>

<%  %> 

  <ul>
    <% dataAll.forEach((x, i) => { %>
      <li>
        <h4 class="war-subject"><%= x.text %></h4>
        <p class="paragraph">tweet id: <%= x.id %>  </p>
        <p class="paragraph">author id: <%= x.author_id %>  </p>
        <br>
        <div class="form-buttons">
          <form action="" method="get">
            <input  name="text" value="<%= x.text %> " type="text" />
            <input  name="tweetid" value="<%= x.id %> " type="text" />
            <input  name="userid" value="<%= user.id %> " type="text" />
            <input  name="source" value="<%= x.author_id %> " type="text" />
            <button class="edit-btn" type="submit">add tweet</button>
          </form>
      </div>
      </li>
    <%  }) %> 
  </ul>
</div>
